.navigationBar
	width 100%
	height 64px
	background-image linear-gradient(to right, #FF1493, #FF8C69)
	position fixed
	top 0
	z-index 9
	.navigationBar_title
		position absolute
		left 50%
		top 60%
		transform translate(-50%,-50%)
	.navigationBar_img
		width 30px
		position absolute
		left 10px
		top 40%
.search_title
	margin-top 64px
	color red
	font-size 20px
	font-weight 700
	height 60px
	width 100%
	line-height 60px
	text-align center
	border-bottom 1px solid #bab7bd
.search_all
	padding 5px 
	.search_main
		width 95%
		padding 5px
		margin-top 10px
		display flex
		font-size 14px
		background-color #FF8C69
		border-radius 5px
		box-shadow 0 0 2px black
		.search_main_img
			border-radius 5px
			width 30px
		.search_main_name
			color red	
			width 60px
			text-align center
		.search_main_val
			flex 1


